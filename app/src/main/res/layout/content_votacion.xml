<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nestedSecrollViewVotacion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.ncodata.votar.VotacionActivity"
    tools:showIn="@layout/activity_votacion">


    <LinearLayout
        android:id="@+id/linearLayoutVotacion"
        android:layout_width="match_parent"
        android:nestedScrollingEnabled="false"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <TextView
            android:id="@+id/textoParrafo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/text_margin"
            android:text="@string/texto_default" />


    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
